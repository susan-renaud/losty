<h1>Found Something?</h1>

<%= simple_form_for(@item) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :question1 %>
  <%= f.input :question2 %>
  <%= f.input :question3 %>
  <%= f.submit class: 'btn btn-success' %>
<% end %>

